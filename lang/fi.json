{
    "language": "Finnish",  // title case
    "native_language": "Suomi",  // title case
    "code": "fi",  // lowercase

    // These Wikitionary URLs are not required by `Extract` but it's nice to
    // have them here for safe-keeping.
    "en_url": "https://en.wiktionary.org/wiki/Category:Finnish_language",
    "lang_url": "https://fi.wiktionary.org",

    // This specifies the first letter of the target language's alphabet.
    "first_letter": "A", 

    // "AFFIXES" lists the names of different types of affixes (sentence case)
    // in English AND in the target language. This list is used to determine if
    // a particular string is a word or an affix. Note that any string whose
    // category is not an affix is considered a word. Only the categories
    // listed here will be considered affixes in `Extract`.
    "AFFIXES": [
        "Affix",
        "Affiksi",          // affix
        "Prefix",
        "Prefiksit",        // prefix
        "Infix",
        "Infiksit",         // infix
        "Interfix",
        "Suffix",
        "Suffiksit",        // suffix
        "Particle",
        "Partikkeli",       // particle
        "Clitic",
        "Liitepartikkeli"   // clitic
    ],

    // "POS" includes a mapping from part-of-speech categories (sentence case)
    // to part-of-speech tags. `Extract` will only extract words with the POS
    // categories mapped below.
    "POS": {
        "Adjective": "ADJ",
        "Adverb": "ADV",
        "Conjunction": "CONJ",
        "Interjection": "UH",
        "Noun": "N",
        "Numeral": "NUM",
        "Ordinal number": "ORD",
        "Postposition": "POST",
        "Preposition": "P",
        "Pronoun": "PRO",
        "Verb": "V"
    }

    // ALL PARTS OF SPEECH (obtained from `Extract.get_likely_pos()`):
    // Abbreviation
    // Acronym
    // Adjective
    // Adverb
    // Compound             ?
    // Conjunction
