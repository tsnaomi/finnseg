{
    "language": "Finnish",  // title case
    "native_language": "Suomi",  // title case
    "code": "fi",  // lowercase

    // These Wikitionary URLs are not required by `Extract` but it's nice to
    // have them here for safe-keeping.
    "en_url": "https://en.wiktionary.org/wiki/Category:Finnish_language",
    "lang_url": "https://fi.wiktionary.org",

    // This specifies the first of the target language's alphabet.
    "first_letter": "A", 

    // "AFFIXES" lists the names of different types of affixes (lowercase) in
    // English AND in the target language. This list is used to determine if a
    // particular string is a word or an affix: Any string whose category is not
    // an affix is considered a word. Only the categories listed here will be
    // considered affixes in `Extract`.
    "AFFIXES": [
        "prefix",
        "prefiksit",        // prefix
        "infix",
        "infiksit",         // infix
        "suffix",
        "suffiksit",        // suffix
        "particle",
        "partikkeli",       // particle
        "clitic",
        "liitepartikkeli"   // clitic
    ],

    // "POS" includes a mapping from part-of-speech categories (lowercase)
    // to part-of-speech tags. `Extract` with only extract words with the POS
    // categories mapped below.
    "POS": {
        "adjective": "ADJ",
        "adverb": "ADV",
        "conjunction": "CONJ",
        "interjection": "UH",
        "noun": "N",
        "numeral": "NUM",
        "postposition": "POST",
        "preposition": "P",
        "pronoun": "PRO",
        "verb": "V"

        // // Ignoring...
        // "prefix"
        // "infix"
        // "suffix"    
        // "proper noun"
        // "phrase"
        // "proverb"
        // "clitic"
        // "particle"
        // "root"
        // "abbreviation"
        // "acronym"
        // "initialism"
    }
}