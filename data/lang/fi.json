{
    "language": "Finnish",  // title case
    "code": "fi",  // lowercase

    // These Wikitionary URLs are not required by `Extract` but it's nice to
    // have them here for safe-keeping.
    "en_url": "https://en.wiktionary.org/wiki/Category:Finnish_language",
    "lang_url": "https://fi.wiktionary.org",

    // This specifies the minimum length of an alphabetic sequence that an
    // extracted word may have. For instance, if `min_word` is set to 3, a
    // words like 'en' or 'a-b-c' will not get exctracted.
    "min_word": 3,

    // This specifies the first of the target language's alphabet.
    "first_letter": "A", 

    // "AFFIXES" lists the names of different types of affixes (lowercase) in
    // English AND  in the target language. This list is used to determine if
    // a particular string is a word or an affix. Any affix type that is not
    // listed here will not be considered an affix in `Extract`.
    "AFFIXES": [
        "prefix",
        "prefiksit",    // prefix
        "infix",
        "infiksit",     // infix
        "suffix",
        "suffiksit"     // suffix
    ],

    // "POS" includes a mapping from part-of-speech categories (lowercase)
    // to part-of-speech tags. `Extract` with only extract words with the POS
    // categories mapped below.
    "POS": {
        "adjective": "ADJ",
        "adverb": "ADV",
        "conjunction": "CONJ",
        "interjection": "UH",
        "noun": "N",
        "numeral": "NUM",
        "postposition": "POST",
        "preposition": "P",
        "pronoun": "PRO",
        "verb": "V"

        // // Ignoring...
        // "prefix"
        // "infix"
        // "suffix"    
        // "proper noun"
        // "phrase"
        // "proverb"
        // "clitic"
        // "particle"
        // "root"
        // "abbreviation"
        // "acronym"
        // "initialism"
    }
}